<h2>
  <%= @profile["Label"] %>
  <span class="subtitle">
    <%= @profile["School"] %>
  </span>
</h2>
<section class="student-strengths">
<% @profile.strengths.each do |strength_set| %>
  <article>
    <header>
      <%= strength_set[:name] %>
    </header>
    <dl>
      <% strength_set[:strengths].each do |strength| %>
        <dt>
          <%= strength["Name"] %>
          <img src="<%= strength["Icon"].first["thumbnails"]["large"]["url"] %>" alt="<%= strength["Name"] %>" />
        </dt>
        <dd>
          <p><%= strength["english"] %></p>
        </dd>
      <% end %>
    </dl>
  </article>
<% end %>
</section>
